<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.ezd.dao.BrowseDao">

    <!--通过<resultMap>映射实体类属性名和表的字段名对应关系 -->

    <resultMap id="browse" type="Browse">
        <!-- 用id属性来映射主键字段 -->
        <id property="browseId" column="browseId"></id>
        <!-- 用result属性来映射非主键字段 -->
        <association property="users" resultMap="edu.ezd.dao.UsersDao.users"></association>
        <association property="article" resultMap="edu.ezd.dao.ArticleDao.article"></association>
    </resultMap>

    <insert id="addBrowse" parameterType="browse">
        INSERT into ezd_browse(BrowseArticle,BrowsePeople) VALUES (#{users.userId},#{article.articleId})
    </insert>



</mapper>
